<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffGiverSetDef>
		<defName>OrganicAgeless</defName>
		<hediffGivers>
			<!-- Environmental -->
			<li Class="HediffGiver_Bleeding">
				<hediff>BloodLoss</hediff>
			</li>
			<li Class="HediffGiver_Hypothermia">
				<hediff>Hypothermia</hediff>
			</li>
			<li Class="HediffGiver_Heat">
				<hediff>Heatstroke</hediff>
			</li>
			
			<!-- Responsive and special -->
			<li Class="HediffGiver_BrainInjury">
				<hediff>TraumaSavant</hediff>
				<partsToAffect>
					<li>Brain</li>
				</partsToAffect>
				<chancePerDamagePct>0.12</chancePerDamagePct>
				<letterLabel>Trauma savant</letterLabel>
				<letter>[PAWN_nameDef]'s brain injury has caused [PAWN_pronoun] to develop strange savant-like abilities.</letter>
			</li>
		</hediffGivers>
	</HediffGiverSetDef>
	
	<HediffGiverSetDef>
		<defName>NecronBasic</defName>
		<hediffGivers>
			<li Class="O21Toolbox.HediffGiver_Racial">
				<hediff>NecrodermisRepair</hediff>
			</li>
		</hediffGivers>
	</HediffGiverSetDef>
	
	<HediffGiverSetDef>
		<defName>OrkBasic</defName>
		<hediffGivers>
			<li Class="O21Toolbox.HediffGiver_Racial">
				<hediff>OrkoidHealing</hediff>
			</li>
			<li Class="O21Toolbox.HediffGiver_Racial">
				<hediff>OrkoidSpores</hediff>
			</li>
		</hediffGivers>
	</HediffGiverSetDef>

	<HediffGiverSetDef>
    <defName>O21_Construct</defName>
		<hediffGivers>
			<li Class="HediffGiver_Bleeding">
				<hediff>O21_Construct_CoolantLoss</hediff>
			</li>
		</hediffGivers>
  	</HediffGiverSetDef>
	
</Defs>
