	<!-- ====== Armors ====== -->
	
	<ThingDef ParentName="TauArmorBase">
		<defName>TauCombatArmorSacea</defName>
		<label>tau combat armor (sa'cea)</label>
		<graphicData>
			<texPath>Things/Tau/Sa'cea/Armour/Sa'ceaArmor</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<apparel>
			<tags Inherit="false">
				<li>SaceaApparel</li>
				<li>SaceaCombatApparel</li>
			</tags>
			<wornGraphicPath>Things/Tau/Sa'cea/Armour/Sa'ceaArmor</wornGraphicPath>
		</apparel>
	</ThingDef>
	
	<ThingDef ParentName="TauLightArmorBase"> <!-- Tau Scout Armour -->
