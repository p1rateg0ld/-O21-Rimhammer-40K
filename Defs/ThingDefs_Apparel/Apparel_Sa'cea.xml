			<texPath>Things/Tau/Sa'cea/Armour/Sa'ceaArmorScout</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<apparel>
			<tags Inherit="false">
				<li>SaceaApparel</li>
				<li>SaceaScoutApparel</li>
			</tags>
			<wornGraphicPath>Things/Tau/Sa'cea/Armour/Sa'ceaArmorScout</wornGraphicPath>
		</apparel>
	</ThingDef>
	
