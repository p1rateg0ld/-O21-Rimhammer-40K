		<statBases>
			<MarketValue>10</MarketValue>
			<Mass>1</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="O21_Orkoid_BodyPartNaturalBase">
		<defName>O21_OrkoidLung</defName>
		<label>orkoid lung</label>
		<description>A biological orkoid lung. A pair of these form the core of the human respiratory system.</description>
		<statBases>
			<MarketValue>10</MarketValue>
			<Mass>1</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="O21_Orkoid_BodyPartNaturalBase">
		<defName>O21_OrkoidKidney</defName>
		<label>orkoid kidney</label>
		<description>A biological orkoid kidney. Filters and removes waste products from the blood.</description>
		<statBases>
			<MarketValue>10</MarketValue>
			<Mass>1</Mass>
		</statBases>
	</ThingDef>

	<ThingDef ParentName="O21_Orkoid_BodyPartNaturalBase">
		<defName>O21_OrkoidLiver</defName>
		<label>orkoid liver</label>
