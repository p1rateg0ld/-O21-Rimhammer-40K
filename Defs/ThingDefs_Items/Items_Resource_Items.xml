<Defs>

	<!-- EGGS -->
	
	<ThingDef ParentName="EggFertBase">
    <defName>EggKnarlocFertilized</defName>
    <label>knarloc egg (fert.)</label>
    <description>Fertilized knarloc egg. If all goes well, it should hatch into a baby knarloc. Mom won't be happy if you snatch it away.</description>
    <graphicData>
		<color>(227,246,230)</color>
		<drawSize>1.6</drawSize>
    </graphicData>
    <statBases>
		<MarketValue>140.0</MarketValue>
    </statBases>
    <comps>
		<li Class="CompProperties_Hatcher">
			<hatcherDaystoHatch>15</hatcherDaystoHatch>
			<hatcherPawn>Knarloc</hatcherPawn>
		</li>
    </comps>
  </ThingDef>
  
